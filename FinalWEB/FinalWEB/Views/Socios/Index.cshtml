@model IEnumerable<FinalWEB.Models.Socios>

@{
    ViewBag.Title = "Socios";
}

<h2>Socios</h2>

<p>
    @Html.ActionLink("Nuevo Registro", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Foto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sexo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Edad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Nacimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Afiliados)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Membresia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lugar_Trabajo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Direc_Oficina)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tel_Oficina)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Ingreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Salida)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cedula)
        </td>
        <td>
            <img src="@Url.Content(item.Foto)" width="100" />
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sexo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Edad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Nacimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Afiliados)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Membresia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lugar_Trabajo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Direc_Oficina)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tel_Oficina)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Ingreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Salida)
        </td>
        <td>
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Inactivar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
